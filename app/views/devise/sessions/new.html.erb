
  <%= form_for(resource, :builder => DeviseBuilder, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <%= f.email_field :email, :autofocus => true, label: :email %>

    <%= f.password_field :password, label: :password %>

    <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me, label: 'Remember me' %>
    <% end -%>

    <%= f.submit_float "Enviar"%>
  <% end %>
