<% toolbar = Theme::Toolbar.new %>
<% toolbar.add_button 'Nuevo', new_type_category_path, 'plus' %>
<% toolbar.add_button 'Ver', type_category_path(@type_category), 'eye' %>
<% toolbar.add_button 'Editar', edit_type_category_path(@type_category), 'edit' %>
<% toolbar.add_button 'Lista', type_categories_path, 'view-list-alt' %>
<% toolbar.add_button 'Eliminar', type_category_path(@type_category), 'delete' %>

<%= render '/layouts/block_header' %>

<%= card ['Type Categories', 'Detalles'], toolbar do %>

  <%= card_text_field_readonly @type_category.name, label: 'Name' %>


<% end %>
